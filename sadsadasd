local TargetTab = Window:MakeTab({
	Name = "Target",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

local locktarget = ""

TargetTab:AddTextbox({
    Name = "Username ",
    Default = "",
    TextDisappear = false,
    Callback = function(val)
        -- Checks if the box doesn't have any text so it doesn't bug and target random person
        if val == "" then
            return
        end
         locktarget = findtarget(val)
        OrionLib:MakeNotification({
            Name = "Shadow GUI",
            Content = "Target Found: " ..locktarget,
            Image = "rbxassetid://4483345998",
            Time = 5
        })
    end
})




TargetTab:AddButton({
	Name = "Goto",
	Callback = function()
		if game.Players:FindFirstChild(locktarget) then
			game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(game:GetService("Players")[locktarget].Character.HumanoidRootPart.Position) * CFrame.new(0, 0, 2)
		 elseif not game.Players:FindFirstChild(locktarget) then
			OrionLib:MakeNotification({
				Name = "Shadow GUI",
				Content = "Player not found",
				Image = "rbxassetid://4483345998",
				Time = 5
			})
		end
	 end
})

TargetTab:AddButton({
	Name = "bring",
	Callback = function()

bringsavepos = game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.Position
game.Workspace.Camera.CameraSubject = game.Players[locktarget].Character.Humanoid
bringplr = true
noclip = true

repeat task.wait()
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Players[locktarget].Character.HumanoidRootPart.CFrame * CFrame.new(0,-5,0)
if not game.Players.LocalPlayer.Character:FindFirstChild("Combat") then
    game.Players.LocalPlayer.Backpack:FindFirstChild("Combat").Parent = game.Players.LocalPlayer.Character
    else
    game.Players.LocalPlayer.Character:FindFirstChild("Combat"):Activate()
end
until game.Players[locktarget].Character.BodyEffects:FindFirstChild("K.O").Value == true or bringplr == false
repeat 
    game:GetService("ReplicatedStorage").MainEvent:FireServer("Grabbing")
    game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(bringsavepos)
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(game:GetService("Players")[locktarget].Character.UpperTorso.Position) * CFrame.new(0,3,0)
    wait(.1)
until game.Players[locktarget].Character:FindFirstChild("GRABBING_CONSTRAINT") or bringplr == false
wait(1)
wait(1.5)
game.Workspace.Camera.CameraSubject = game.Players[locktarget].Character.Humanoid
bringplr = false
noclip = false
game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(bringsavepos)
game.Workspace.Camera.CameraSubject = game.Players.LocalPlayer.Character.Humanoid
	 end
})

TargetTab:AddToggle({
	Name = "View",
	Default = false,
	Callback = function(bool)
		if bool then

			viewingplr = true
		   
		   game.Workspace.Camera.CameraSubject = game.Players[locktarget].Character.Humanoid;
		 
		   game.Players[locktarget].CharacterAdded:connect(function()
			   game.Players.LocalPlayer.Character:WaitForChild("FULLY_LOADED_CHAR")
			 wait(1)
			 if viewingplr == true then
			   game.Workspace.Camera.CameraSubject = game.Players[locktarget].Character.Humanoid;
			 end
		   end)

		else
      
      viewingplr = false
game.Workspace.Camera.CameraSubject = game.Players.LocalPlayer.Character.Humanoid
		end
	end
})

calltarget = false

TargetTab:AddToggle({
	Name = "Spam Call",
	Default = false,
	Callback = function(bool)
		if bool then

			calltarget = true
			while calltarget == true do
				game.Players.LocalPlayer.Character:FindFirstChildOfClass("Humanoid"):UnequipTools()
				wait()
				if game.Players.LocalPlayer.Backpack:FindFirstChild("[Phone]") then
					game.Players.LocalPlayer.Backpack:FindFirstChild("[Phone]").Parent = game.Players.LocalPlayer.Character
				end
				wait()
				game:GetService("ReplicatedStorage").MainEvent:FireServer("PhoneCall", locktarget)
				wait(.5)
			end
			  else
				calltarget = false
			  end
			end
})




-- Define variables
-- Define variables
local isbangenabled = false
local bangsavepos = nil

-- Callback function for the toggle button
TargetTab:AddToggle({
    Name = "Bang",
    Callback = function(bool)
        if bool then
            if not isbangenabled then
                -- Save current position of LocalPlayer's HumanoidRootPart
                bangsavepos = game.Players.LocalPlayer.Character.HumanoidRootPart.Position
                
                -- Play Bang animation
                local player = game.Players.LocalPlayer
                local BangAnim = Instance.new("Animation")
                BangAnim.AnimationId = "rbxassetid://3119980985"
                local BangAnimPlay = player.Character:WaitForChild("Humanoid"):LoadAnimation(BangAnim)
                BangAnimPlay:Play()
                BangAnimPlay:AdjustSpeed(2)
                
                -- Set isbangenabled to true
                isbangenabled = true
                
                -- Connect Stepped event to update Bang action position
                game:GetService('RunService').Stepped:Connect(function()
                    if isbangenabled and locktarget then
                        local targetPlayer = game:GetService("Players")[locktarget]
                        if targetPlayer and targetPlayer.Character and targetPlayer.Character:FindFirstChild("HumanoidRootPart") then
                            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = targetPlayer.Character.HumanoidRootPart.CFrame * CFrame.new(0, 0, 1)
                        end
                    end
                end)
                
            end
        else
            -- If the toggle is turned off, reset Bang action
            if isbangenabled then
                -- Reset position of LocalPlayer's HumanoidRootPart
                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(bangsavepos)
                
                -- Stop Bang animation if it's playing
                for _, v in pairs(game.Players.LocalPlayer.Character.Humanoid:GetPlayingAnimationTracks()) do
                    if v.Animation.AnimationId == "rbxassetid://3119980985" then
                        v:Stop()
                    end
                end
                
                -- Set isbangenabled to false
                isbangenabled = false
            end
        end
    end
})

-- Initialize isautokillenabled
-- Initialize isautokillenabled
-- Initialize isautokillenabled
-- Initialize isautokillenabled
local isautokillenabled = false

-- Function to toggle autokill
TargetTab:AddToggle({
    Name = "Autokill",
    Default = false,
    Callback = function(bool)
        isautokillenabled = bool  -- Update isautokillenabled based on UI toggle
    end
})

-- Connect to RenderStepped event
game:GetService("RunService").RenderStepped:Connect(function()
    if isautokillenabled and game.Players[locktarget] and game.Players[locktarget].Character then
        local targetCharacter = game.Players[locktarget].Character
        local targetRootPart = targetCharacter:FindFirstChild("HumanoidRootPart")

        if targetRootPart then
            -- Calculate predicted position
            local predictedPosition = targetRootPart.Position + targetRootPart.Velocity * 0.36

            if targetCharacter.BodyEffects["K.O"].Value == false then
                -- Execute this block when the target player is not KO'd
                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(predictedPosition) * CFrame.new(0, -3, 0)
                if game.Players.LocalPlayer.Character:FindFirstChild('Combat') then
                    game.Players.LocalPlayer.Character:FindFirstChild('Combat'):Activate()
                else
                    game.Players.LocalPlayer.Backpack:FindFirstChild('Combat').Parent = game.Players.LocalPlayer.Character
                end
            else
                -- Execute this block when the target player is KO'd
                if targetCharacter.BodyEffects.Dead.Value == false then
                    local upperTorso = targetCharacter:FindFirstChild("UpperTorso") or targetCharacter:FindFirstChild("Torso")

                    if upperTorso then
                        local targetUpperTorsoPosition = upperTorso.Position + Vector3.new(0, 3, 0)

                        -- Teleport the local player character to the target's UpperTorso position with an offset
                        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(targetUpperTorsoPosition)

                        -- Wait a short time to ensure the local player's position is updated before firing the server event
                        wait(0.1)  -- Adjust delay as needed

                        game.ReplicatedStorage.MainEvent:FireServer("Stomp")
                    else
                        -- If UpperTorso (or Torso) is not found, fallback to using the targetRootPart position with offset
                        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(predictedPosition) * CFrame.new(0, 3, 0)
                        wait(0.1)
                        game.ReplicatedStorage.MainEvent:FireServer("Stomp")
                    end
                else
                    -- Teleport the local player far underground if the target player is dead
                    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(0, -1000, 0) -- Adjust coordinates to be underground
                end
            end
        end
    end
end)


local isAutoSaveEnabled = false 
local autoSaveThread = nil

TargetTab:AddToggle({
    Name = "Auto-Save",
    Callback = function(bool)
        if bool then
            isAutoSaveEnabled = true
            autoSaveThread = coroutine.create(function()
                while isAutoSaveEnabled do
                    if game.Players[locktarget] then
                        local targetPlayer = game.Players[locktarget]
                        
                        if targetPlayer.Character and targetPlayer.Character.BodyEffects and targetPlayer.Character.BodyEffects["K.O"] then
                            if targetPlayer.Character.BodyEffects["K.O"].Value == true then
                                game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(targetPlayer.Character.UpperTorso.Position) * CFrame.new(0, 1, 0)
                                wait(0.4)
                                game.ReplicatedStorage.MainEvent:FireServer("Grabbing")
                                wait(0.4)
                                game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(Vector3.new(-484.67132568359, 41.499858856201, -100.71377563477))
                                wait(1.5)
                                game.ReplicatedStorage.MainEvent:FireServer("Grabbing")
                            end
                        else
                            isAutoSaveEnabled = false
                        end
                    else
                        isAutoSaveEnabled = false
                    end
                    
                    wait(5) 
                end
            end)
            
            coroutine.resume(autoSaveThread)
        else
            isAutoSaveEnabled = false
            if autoSaveThread then
                coroutine.yield(autoSaveThread)
            end
        end
    end
})



TargetTab:AddButton({
    Name = "Stomp Player",
    Callback = function()
        local targetPlayer = game.Players:FindFirstChild(locktarget)
        
        if targetPlayer and targetPlayer.Character then
            local bodyEffects = targetPlayer.Character:FindFirstChild("BodyEffects")
            local koEffect = bodyEffects and bodyEffects:FindFirstChild("K.O")
            
            if koEffect and koEffect.Value == true then
                local playerCharacter = game.Players.LocalPlayer.Character
                local playerRootPart = playerCharacter and playerCharacter:FindFirstChild("HumanoidRootPart")
                
                if playerRootPart then
                    local originalPosition = playerRootPart.Position
                    
                    playerRootPart.CFrame = CFrame.new(targetPlayer.Character.UpperTorso.Position) * CFrame.new(0, 1, 0)
                    
                    wait(0.4)
                    
                    game.ReplicatedStorage.MainEvent:FireServer("Stomp")
                    
                    wait(2)
                    
                    playerRootPart.CFrame = CFrame.new(originalPosition)
                end
            else
                Notif("Target is not knocked.", 10)
            end
        end
    end
})







local isFlingEnabled = false  -- Variable to track if fling is enabled
local Flinging
local Noclip

local function FlingYes()
    -- Fling logic here
    -- Example logic: You can customize this based on your specific requirements
    local dis = 3
    local IncreaseThing = 6
    
    Noclip = game:GetService("RunService").Stepped:Connect(function()
        for _, v in pairs(game.Players.LocalPlayer.Character:GetDescendants()) do
            if v:IsA('BasePart') then
                v.CanCollide = false
            end
        end
    end)
    
    Flinging = game:GetService("RunService").Heartbeat:Connect(function()
        task.wait(.01)
        if game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart") and game.Players[locktarget] then
            local XCFrameChange = 0
            local ZCFrameChange = 0
            
            if game.Players[locktarget].Character.Humanoid.MoveDirection.X < 0 then
                XCFrameChange = -IncreaseThing
            elseif game.Players[locktarget].Character.Humanoid.MoveDirection.X > 0 then
                XCFrameChange = IncreaseThing
            end
            
            if game.Players[locktarget].Character.Humanoid.MoveDirection.Z < 0 then
                ZCFrameChange = -IncreaseThing
            elseif game.Players[locktarget].Character.Humanoid.MoveDirection.Z > 0 then
                ZCFrameChange = IncreaseThing
            end
            
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(
                game.Players[locktarget].Character.UpperTorso.Position.X + math.random(-dis, dis) + XCFrameChange,
                game.Players[locktarget].Character.UpperTorso.Position.Y,
                game.Players[locktarget].Character.UpperTorso.Position.Z + math.random(-dis, dis) + ZCFrameChange
            ) * CFrame.Angles(
                math.rad(game.Players.LocalPlayer.Character.HumanoidRootPart.Orientation.X + 350),
                math.rad(game.Players.LocalPlayer.Character.HumanoidRootPart.Orientation.Y + 200),
                math.rad(game.Players.LocalPlayer.Character.HumanoidRootPart.Orientation.Z + 240)
            )
        end
    end)
end

local function DisabledFling()
    if Flinging then
        Flinging:Disconnect()
    end
    if Noclip then
        Noclip:Disconnect()
    end
    -- Reset character position after stopping fling
    if saveposfling and game.Players.LocalPlayer.Character then
        for i = 1, 100 do
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(saveposfling)
            task.wait()
        end
    end
    isFlingEnabled = false  -- Reset the variable
end

TargetTab:AddToggle({
    Name = "Fling",
    Callback = function(bool)
        if bool then
            -- Start fling
            saveposfling = game.Players.LocalPlayer.Character.HumanoidRootPart.Position
            FlingYes()
            isFlingEnabled = true
        else
            -- Disable fling completely
            if isFlingEnabled then
                DisabledFling()
            end
        end
    end
})





TargetTab:AddToggle({
    Name = "Mark Player",
    Default = false,
    Callback = function(bool)

        local function createMarker(parent)
            local marker = Instance.new("Part")
            marker.Size = Vector3.new(2, 2, 2) 
            marker.Shape = Enum.PartType.Ball
            marker.Color = Color3.fromRGB(0, 0, 255)
            marker.Transparency = 0.5
            marker.CanCollide = false
            marker.Anchored = true
            marker.Parent = parent
            return marker
        end

        local function updateMarkerPosition(targetPlayer)
            local marker = targetPlayer.Character:FindFirstChild("Marker")
            if marker then
                marker.Position = targetPlayer.Character.HumanoidRootPart.Position
            end
        end

        if bool then
            if game.Players:FindFirstChild(locktarget) then
                local targetPlayer = game.Players[locktarget]

                local marker = targetPlayer.Character:FindFirstChild("Marker")
                if not marker then
                    marker = createMarker(targetPlayer.Character)
                    marker.Name = "Marker"
                end
                updateMarkerPosition(targetPlayer)

                local connection
                connection = game:GetService("RunService").RenderStepped:Connect(function()
                    if game.Players:FindFirstChild(locktarget) then
                        local updatedTargetPlayer = game.Players[locktarget]
                        updateMarkerPosition(updatedTargetPlayer)
                    else
                        connection:Disconnect()
                    end
                end)

            end
        else
            if game.Players:FindFirstChild(locktarget) then
                local targetPlayer = game.Players[locktarget]
                local marker = targetPlayer.Character:FindFirstChild("Marker")
                if marker then
                    marker:Destroy()
                end
            end
        end
    end
})



local BuyTab = Window:MakeTab({
	Name = "AutoBuy",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

for i, v in pairs(game:GetService("Workspace").Ignored.Shop:GetChildren()) do
    BuyTab:AddButton({
        Name = v.Name,
        Callback = function()
            local Pos = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
            local Teleport = v:FindFirstChild("Head")
            if Teleport then
                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = Teleport.CFrame
                local CD = v:FindFirstChild("ClickDetector")
                if CD then
                    wait(0.75)
                    fireclickdetector(CD)
                    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = Pos
                end
            end
        end
    })
end



local FarmTab = Window:MakeTab({
	Name = "Farms",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})









local isFarmEnabled = false

FarmTab:AddToggle({
    Name = "Atm Farm [Cant Be Stoped] [Beta]",
    Callback = function(bool)
        local isFarmEnabled = false  -- Initially, farm is disabled
        
        if bool then
            local plr = game.Players.LocalPlayer

            -- Function to delete anti-cheat scripts from the character
            local function DeleteAntiCheat()
                for i, v in pairs(plr.Character:GetChildren()) do
                    if v.ClassName == "Script" and v.Name ~= "Health" then
                        v:Destroy()
                    end
                end
            end
            
            -- Wait until the character is fully loaded
            repeat wait() until plr.Character:FindFirstChild("FULLY_LOADED_CHAR")
            
            -- Initial setup
            isFarmEnabled = true  -- Enable farm
            local reset = false
            
            -- Delete anti-cheat scripts initially
            DeleteAntiCheat()
            
            -- Connect to CharacterAdded event to ensure anti-cheat scripts are deleted on respawn
            plr.CharacterAdded:Connect(function(character)
                repeat wait() until plr.Character and plr.Character:FindFirstChild("FULLY_LOADED_CHAR")
                DeleteAntiCheat()
            end)
            
            -- Monitor player's health and set reset flag if health drops to 0
            spawn(function()
                while wait() do
                    pcall(function()
                        if plr.Character.Humanoid.Health <= 0 then
                            reset = true
                        end
                    end)
                end
            end)
            
            -- Handle idle kick prevention
            game.Players.LocalPlayer.Idled:Connect(function()
                game:GetService("VirtualUser"):Button2Down(Vector2.new(0,0),workspace.CurrentCamera.CFrame)
                wait(1)
                game:GetService("VirtualUser"):Button2Up(Vector2.new(0,0),workspace.CurrentCamera.CFrame)
            end)
            
            -- Ensure player's humanoid state is maintained
            game:GetService('RunService').Stepped:Connect(function()
                pcall(function()
                    plr.Character.Humanoid:ChangeState(11)
                end)
                setsimulationradius(math.huge)
            end)
            
            -- Main loop to interact with Cashiers and pick up MoneyDrops
            while wait(0.5) do
                -- Check if farm is enabled
                if not isFarmEnabled then
                    break  -- Exit the loop and stop the script
                else
                    for i, v in pairs(game.Workspace.Cashiers:GetChildren()) do
                        if v.Humanoid.Health > 0 then
                            spawn(function()
                                while v.Humanoid.Health > 0 do
                                    wait()
                                    pcall(function()
                                        plr.Character.HumanoidRootPart.CFrame = v.Head.CFrame * CFrame.new(0, -2, 1.5)
                                    end)
                                end
                            end)
                            repeat
                                pcall(function()
                                    plr.Character.Humanoid:EquipTool(plr.Backpack.Combat)
                                end)
                                wait(0.1)
                                pcall(function()
                                    plr.Character.Combat:Activate()
                                end)
                            until v.Humanoid.Health <= 0
                            wait(0.1)
            
                            for ii, vv in pairs(game.Workspace.Ignored.Drop:GetChildren()) do
                                if vv.Name == "MoneyDrop" then
                                    if (plr.Character.HumanoidRootPart.Position - vv.Position).Magnitude <= 25 then
                                        vv.Name = "Grabbing"
                                        vv.Anchored = true
                                        while game.Workspace.Ignored.Drop:FindFirstChild("Grabbing") do
                                            wait()
                                            pcall(function()
                                                plr.Character.HumanoidRootPart.CFrame = vv.CFrame
                                                if not reset then
                                                    game.Players.LocalPlayer.Character:FindFirstChildOfClass("Humanoid"):UnequipTools()
                                                    fireclickdetector(vv.ClickDetector)
                                                end
                                            end)
                                        end
                                    end
                                end
                            end
                        end
                    end
                end
            end
        else
            isFarmEnabled = false  -- Disable farm
        end
    end
})



local weightfarm = false
local activeHeavyWeights = nil -- Variable to store the currently equipped HeavyWeights tool

FarmTab:AddToggle({
    Name = "Weight Farm",
    Callback = function(bool)
        if bool then
            weightfarm = true
            
            -- Attempt to buy and equip HeavyWeights tool
            buy("[HeavyWeights] - $265", "[HeavyWeights]", 265) -- Replace with your buy function
            
            -- Check if HeavyWeights tool is in backpack and equip it
            local heavyWeights = game.Players.LocalPlayer.Backpack:FindFirstChild("[HeavyWeights]")
            if heavyWeights then
                heavyWeights.Parent = game.Players.LocalPlayer.Character
                activeHeavyWeights = heavyWeights
                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-319.2906494140625, 80.41372680664062, -210.28460693359375)
            end
            
            -- Start the weight farming loop
            while weightfarm and activeHeavyWeights do
                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(56.169578552246, 11.749998092651, 117.1457824707)
                wait(0.1)
                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-68.71996307373, 11.749997138977, 118.62348175049)
                wait(0.1)
                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-65.490776062012, 11.749997138977, 274.65905761719)
                wait(0.1)
                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(72.370185852051, 11.749997138977, 273.08349609375)
                wait(0.1)
                activeHeavyWeights:Activate()
                wait(0.5)
                activeHeavyWeights:Deactivate()
                wait(0.5)
            end
        else
            weightfarm = false
            
            -- Cleanup when Weight Farming is disabled
            if activeHeavyWeights then
                activeHeavyWeights.Parent = game.Players.LocalPlayer.Backpack
                activeHeavyWeights = nil
            end
        end
    end
})



local stompall = false
local stompallSavePos = nil

FarmTab:AddToggle({
  Name = "Stomp Farm",
  Default = false,
  Callback = function(bool)
    if bool then
      stompallSavePos = game.Players.LocalPlayer.Character.HumanoidRootPart.Position
      stompall = true
    else
      stompall = false
      -- Optional: Return to the saved position when stopping the farm
      if stompallSavePos then
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(stompallSavePos)
      end
    end
  end
})

game:GetService("RunService").RenderStepped:Connect(function()
  if stompall then
    -- Perform the stomp action
    game:GetService("ReplicatedStorage").MainEvent:FireServer("Stomp")

    for _, player in ipairs(game.Players:GetPlayers()) do
      if player ~= game.Players.LocalPlayer and player.Character then
        local bodyEffects = player.Character:FindFirstChild("BodyEffects")
        if bodyEffects then
          local KOValue = bodyEffects:FindFirstChild("K.O")
          if KOValue and KOValue:IsA("BoolValue") and KOValue.Value then
            -- Check if the player is knocked out
            if not bodyEffects:FindFirstChild("Dead") or not bodyEffects.Dead.Value then
              -- Move to the knocked-out player
              game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(player.Character.UpperTorso.Position) * CFrame.new(0, 3, 0)
            else
              -- Move to a default position and block if the player is dead
              game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-116, -58, 146)
              game.ReplicatedStorage.MainEvent:FireServer("Block", true)
            end
          end
        end
      end
    end
  end
end)




-- Define a variable to track active hospital farm automation
local activeHospitalFarm = false

-- AddToggle function to toggle hospital farm automation
FarmTab:AddToggle({
    Name = "Hospital Farm",
    Callback = function(bool)
        activeHospitalFarm = bool
        if activeHospitalFarm then
            while activeHospitalFarm do
                wait(1)  -- Adjust the wait time as needed
                
                -- Loop through each child in HospitalJob
                for i, v in pairs(game:GetService("Workspace").Ignored.HospitalJob:GetChildren()) do
                    -- Check if the object name contains specific strings
                    if v.Name:find("Can I get the Red bottle") then
                        repeat
                            wait()
                            game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(game:GetService("Workspace").Ignored.HospitalJob.Red.Position)
                            wait()
                            fireclickdetector(v.ClickDetector)
                        until v.Name:find("Thank you!")
                    elseif v.Name:find("Can I get the Blue bottle") then
                        repeat
                            wait()
                            game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(game:GetService("Workspace").Ignored.HospitalJob.Blue.Position)
                            wait()
                            fireclickdetector(game:GetService("Workspace").Ignored.HospitalJob.Blue.ClickDetector)
                            wait()
                            if v:FindFirstChild("Head") then
                                game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(v.Head.Position)
                                wait()
                            end
                            fireclickdetector(v.ClickDetector)
                        until v.Name:find("Thank you!")
                    elseif v.Name:find("Can I get the Green bottle") then
                        wait()
                        game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(game:GetService("Workspace").Ignored.HospitalJob.Green.Position)
                        wait()
                        fireclickdetector(game:GetService("Workspace").Ignored.HospitalJob.Green.ClickDetector)
                        wait()
                        if v:FindFirstChild("Head") then
                            game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(v.Head.Position)
                            wait()
                        end
                        fireclickdetector(v.ClickDetector)
                    else
                        -- Default action if none of the specific names are matched
                        game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(56.169578552246, 11.749998092651, 117.1457824707)
                        wait()
                        game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-68.71996307373, 11.749997138977, 118.62348175049)
                        wait()
                        game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-65.490776062012, 11.749997138977, 274.65905761719)
                        wait()
                        game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(72.370185852051, 11.749997138977, 273.08349609375)
                        wait()
                    end
                end
                
                fireclickdetector(game:GetService("Workspace").Ignored.HospitalJob.Red.ClickDetector)
                wait()
                local v = game:GetService("Workspace").Ignored.HospitalJob:FindFirstChild("Patient")
                if v and v:FindFirstChild("Head") then
                    game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(v.Head.Position)
                end
            end
        end
    end
})









