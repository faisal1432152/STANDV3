			end
		end
    end
	})


---- Combat Tab

local CombatTab = Window:MakeTab({
	Name = "Combat",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

CombatTab:AddButton({
	Name = "Inf Jump",
	Callback = function()
			local player = game.Players.LocalPlayer
			local spacedown = false
			staminup = true
			game:GetService('UserInputService').InputBegan:Connect(function(key,b)
				if key.KeyCode == Enum.KeyCode.Space and not b then
					if staminup == true then
						spacedown = true
						while spacedown == true do
							wait()
							player.Character:FindFirstChildWhichIsA('Humanoid').JumpPower = 50
							player.Character:FindFirstChildWhichIsA('Humanoid').Jump = true
							player.Character:FindFirstChildWhichIsA('Humanoid').JumpPower = 50
						end
					end
				end
			end)
			game:GetService('UserInputService').InputEnded:Connect(function(key,b)
				if key.KeyCode == Enum.KeyCode.Space and not b then
					if staminup == true then
						spacedown = false
					end
				end
			end)
		end
})

local antiSlowConnection

CombatTab:AddToggle({
    Name = "Anti-Slow",
    Default = false,
    Callback = function(gh)
        if gh then
            antiSlowConnection = game:GetService('RunService'):BindToRenderStep("Anti-Slow", 0, function()
                local player = game.Players.LocalPlayer
                local movementEffects = player.Character.BodyEffects.Movement

                if movementEffects:FindFirstChild("NoWalkSpeed") then
                    movementEffects.NoWalkSpeed:Destroy()
                end
                if movementEffects:FindFirstChild("ReduceWalk") then
                    movementEffects.ReduceWalk:Destroy()
                end
                if movementEffects:FindFirstChild("NoJumping") then
                    movementEffects.NoJumping:Destroy()
                end
                if player.Character.BodyEffects.Reload.Value then
                    player.Character.BodyEffects.Reload.Value = false
                end
            end)
        else
            if antiSlowConnection then
                game:GetService('RunService'):UnbindFromRenderStep("Anti-Slow")
                antiSlowConnection = nil
            end
        end
    end  
})


CombatTab:AddToggle({
    Name = "Anti-Stand",
    Default = false,
    Callback = function(isEnabled)
        local RunService = game:GetService("RunService")
        local player = game:GetService("Players").LocalPlayer
        local character = player.Character or player.CharacterAdded:Wait()
        local humanoidRootPart = character:WaitForChild("HumanoidRootPart")

        if isEnabled then
            if not _G.antiStandConnection then
                _G.antiStandEnabled = true
                _G.previousPosition = humanoidRootPart.CFrame 
                
                _G.antiStandConnection = RunService.RenderStepped:Connect(function()
                    local xMin, xMax = -500, 500
                    local yMin, yMax = 300, 500
                    local zMin, zMax = -500, 500
                    
                    local x = math.random(xMin, xMax)
                    local y = math.random(yMin, yMax)
                    local z = math.random(zMin, zMax)
                    humanoidRootPart.CFrame = CFrame.new(x, y, z)
                end)
            end
        else
            if _G.antiStandConnection then
                _G.antiStandEnabled = false
                _G.antiStandConnection:Disconnect() 
                _G.antiStandConnection = nil
                
                if _G.previousPosition then
                    humanoidRootPart.CFrame = _G.previousPosition 
                    _G.previousPosition = nil
                end
            end
        end
    end
})

local function enableGodBlock()
    godBlockLoop = game:GetService("RunService").Heartbeat:Connect(function()
        if game.Players.LocalPlayer.Character and game.Players.LocalPlayer.Character:FindFirstChild("BodyEffects") then
            local bodyEffects = game.Players.LocalPlayer.Character.BodyEffects
            if bodyEffects:FindFirstChild('Block') then
                bodyEffects.Block:Destroy()
            end
            game.ReplicatedStorage.MainEvent:FireServer("Block", true)
            wait()
            game.ReplicatedStorage.MainEvent:FireServer("Block", false)
        end
    end)
end

local function disableGodBlock()
    if godBlockLoop then
        godBlockLoop:Disconnect()
        godBlockLoop = nil
    end
end


CombatTab:AddToggle({
	Name = "God-Block",
	Default = false,
	Callback = function(bool)
        if bool then
            enableGodBlock()
            else
            disableGodBlock()
            end
	end
})


CombatTab:AddToggle({
	Name = "Auto-Stomp",
	Default = false,
	Callback = function(r)
		if r == true then
			game:GetService('RunService'):BindToRenderStep("Auto-Stomp", 0 , function()
				game:GetService("ReplicatedStorage").MainEvent:FireServer("Stomp")
			end)
		elseif r == false then
			game:GetService('RunService'):UnbindFromRenderStep("Auto-Stomp")
		end
	end
})

CombatTab:AddToggle({
	Name = "Auto-Reload",
	Default = false,
	Callback = function(r)
		if r == true then
			game:GetService('RunService'):BindToRenderStep("Auto-Reload", 0 , function()
				if game:GetService("Players").LocalPlayer.Character:FindFirstChildWhichIsA("Tool") then
					if game:GetService("Players").LocalPlayer.Character:FindFirstChildWhichIsA("Tool"):FindFirstChild("Ammo") then
						if game:GetService("Players").LocalPlayer.Character:FindFirstChildWhichIsA("Tool"):FindFirstChild("Ammo").Value <= 0 then
							game:GetService("ReplicatedStorage").MainEvent:FireServer("Reload", game:GetService("Players").LocalPlayer.Character:FindFirstChildWhichIsA("Tool")) 
							wait(1)
						end
					end
				end
			end)
		elseif r == false then
			game:GetService('RunService'):UnbindFromRenderStep("Auto-Reload")
		end
	end
})

CombatTab:AddButton({
Name = "No Recoil",
Callback = function()
	local player = game.Players.LocalPlayer
    for i,v in pairs(game:GetService('Workspace'):GetChildren()) do
        if v:IsA('Camera') then
            v:Destroy()
        end
    end
    local newcam = Instance.new('Camera')
    newcam.Parent = game:GetService('Workspace')
    newcam.Name = 'Camera'
    newcam.CameraType = 'Custom'
    newcam.CameraSubject = player.Character:FindFirstChildWhichIsA('Humanoid')
    newcam.HeadLocked = true
    newcam.HeadScale = 1
end
})


--Teleport Tab--

local TeleportTab = Window:MakeTab({
	Name = "Teleports",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

local TweenService = game:GetService("TweenService")

-- Define the tweening function
local function tweenTo(position)
    local character = game.Players.LocalPlayer.Character
    local humanoidRootPart = character and character:FindFirstChild("HumanoidRootPart")

    if humanoidRootPart then
        local currentPosition = humanoidRootPart.Position
        local distance = (position.Position - currentPosition).Magnitude
        local tweenInfo = TweenInfo.new(distance / 50, Enum.EasingStyle.Quad)

        local tweenGoal = { CFrame = position }
        local tween = TweenService:Create(humanoidRootPart, tweenInfo, tweenGoal)

        local success, errorMessage = pcall(function()
            tween:Play()
            tween.Completed:Wait()  -- Wait for the tween to complete
        end)

        if not success then
            warn("Tween failed: " .. errorMessage)
        end
    else
        warn("HumanoidRootPart not found in character")
    end
end

-- Add buttons with tween functionality
