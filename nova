MoneyTab:Toggle("Cash Aura", function(bool)
    if bool then
      cashaura = true
    else
      cashaura = false
    end
end)

game:GetService('RunService').Stepped:connect(function()
  if cashaura == true then
    if cashauraping == true then
      for i, v in pairs(workspace.Ignored.Drop:GetChildren()) do
        if v.Name == "MoneyDrop" and (game.Players.LocalPlayer.Character.HumanoidRootPart.Position - v.Position).Magnitude < 25 then
          fireclickdetector(v.ClickDetector)
        end
      end
    end

local pingvalue = game:GetService("Stats").Network.ServerStatsItem["Data Ping"]:GetValueString()
local split = string.split(pingvalue,'(')
local ping = tonumber(split[1])
  if ping >= 400 then
    print("Over")
    cashauraping = false
  elseif ping <= 400 then
    print("Under")
    cashauraping = true
end

  end
end)

local dropcashamount = 10000

MoneyTab:TextBox("Drop Cash Amount", false, function(val)
  dropcashamount = val
  Notif("Drop Cash Amount Set To: " ..val, 10)
end)

MoneyTab:Toggle("Auto Drop Cash", function(bool)
    if bool then
      autodrop = true
    else
      autodrop = false
    end
end)

MoneyTab:Toggle("Float Money", function(bool)
    if bool then
      moneyfloat = true
    else
      moneyfloat = false
    end
end)

game:GetService('RunService').Stepped:connect(function()
  if moneyfloat == true then
    pcall(function()
        for i, v in pairs(game:GetService("Workspace").Ignored.Drop:GetChildren()) do
            if v.Name == ("MoneyDrop") then
                if v:IsA('BasePart') then
                    v.Velocity = Vector3.new(0,10,0);
                end;
            end
        end
    end);
  end
end)


game:GetService('RunService').Stepped:connect(function()
  if autodrop == true then
    game.ReplicatedStorage.MainEvent:FireServer("DropMoney", dropcashamount)
  end
end)

