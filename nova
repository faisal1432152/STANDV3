if stompall == true then
game:GetService("ReplicatedStorage").MainEvent:FireServer("Stomp")
for i,v in pairs(game.Players:GetChildren()) do
    if v.Character then
    if v.Character:FindFirstChild("BodyEffects") then
        if v.Character.BodyEffects:FindFirstChild("K.O") then
    if v.Character.BodyEffects:FindFirstChild("K.O").Value == true then
    workspace.CurrentCamera.CameraSubject = v.Character:WaitForChild("UpperTorso")
    if v.Character.BodyEffects:FindFirstChild("Dead").Value == false then
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(v.Character.UpperTorso.Position) * CFrame.new(0,3,0)
    else
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(0,10000000,0)
end
    end
    end
    end
end
end
end
end)
