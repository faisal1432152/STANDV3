SeizureAnimPlay:Play()
SeizureAnimPlay:AdjustSpeed(1.5)

    else

    for i,v in pairs(game.Players.LocalPlayer.Character.Humanoid:GetPlayingAnimationTracks()) do
      if v.Animation.AnimationId == "rbxassetid://5641749824" then
        v:Stop()
      end
    end

end
end)

