-- loading screen destroy
for i = 1,5 do
    if game:GetService("CoreGui"):FindFirstChild("Loading") then
        game:GetService("CoreGui").Loading:Destroy()
    end
end

-- / Exec Logs
getgenv().ScriptName = "Nova Gui " ..NovaVersion
--loadstring(game:HttpGet("https://icxy.xyz/NovaGui/ExecLog", true))()
-- NIGGER EXECUTOR LOGS --
local NotifyLibrary = loadstring(game:HttpGet("https://raw.githubusercontent.com/qvrwd/etarry/main/libraries/notification/akalinotif"))()

-- Notif("Hi", 5)
function Notif(txt, time)
    
local Notify = NotifyLibrary.Notify
Notify({
	Title = "Nova Gui " ..NovaVersion,
	Description = txt,
	Duration = time
})

end


-- keybind toggle ui

repeat task.wait() until game.CoreGui:FindFirstChild("Nova Gui | " ..NovaVersion)

wait(1)
    
local OldSize = game:GetService("CoreGui")["Nova Gui | " ..NovaVersion].Main.Size
UiToggle = false
game.Players.LocalPlayer:GetMouse().KeyDown:connect(function(k)
    if k == getgenv().UiKeybind then
        if UiToggle == false then
            UiToggle = true
            --invis
            game:GetService("TweenService"):Create(game:GetService("CoreGui")["Nova Gui | "..NovaVersion].Main,TweenInfo.new(.3,Enum.EasingStyle.Linear),{Size = UDim2.new(0,0,0)}):Play()
        elseif UiToggle == true then
            UiToggle = false
            --vis
            game:GetService("TweenService"):Create(game:GetService("CoreGui")["Nova Gui | "..NovaVersion].Main,TweenInfo.new(.3,Enum.EasingStyle.Linear),{Size = UDim2.new(0, 575 ,  0, 295 )}):Play()
        end
    end
end) 

