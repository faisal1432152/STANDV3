			return
		end
    if clicktpSounds == true then
      play(558640653, true, true)
      task.wait()
      game.Players.LocalPlayer.Character:FindFirstChildOfClass("Humanoid"):UnequipTools()
      task.wait()
      game.Players.LocalPlayer.Backpack:FindFirstChild("Teleport Tool").Parent = game.Players.LocalPlayer.Character
    end
		HRP.CFrame = CFrame.new(Mouse.Hit.X, Mouse.Hit.Y + 3, Mouse.Hit.Z, select(4, HRP.CFrame:components()))
	end)

end)

