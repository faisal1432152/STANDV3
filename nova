local function rm()
    for i, v in pairs(game.Players.LocalPlayer.Character:GetDescendants()) do
        if v.Name == "OriginalPosition" then
            v:Destroy()
        elseif v.Name == "OriginalSize" then
            v:Destroy()
        elseif v.Name == "AvatarPartScaleType" then
            v:Destroy()
        end
    end
end
local Humanoid = game.Players.LocalPlayer.Character.Humanoid

game:service'Players'.LocalPlayer.Character.Humanoid.HealthChanged:Connect(function(arg)
    if arg <= 80 and juggernaut == false then

juggernaut = true

RageAnimPlay:Play()

rm()
wait(0.5)
Humanoid:FindFirstChild("BodyTypeScale"):Destroy()
wait(1)

rm()
wait(0.5)
Humanoid:FindFirstChild("BodyWidthScale"):Destroy()
wait(1)


    end
end)


game:service'Players'.LocalPlayer.Character.Humanoid.HealthChanged:Connect(function(arg)
    if arg == 0 and juggernaut == true then
        juggernaut = false
    end
end)

