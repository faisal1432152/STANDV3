SettingsTab:Toggle("Sound On Button Click", function(bool)
if bool then
    writefile("NovaGui/NovaGuiSettings/SoundsOnClick.nova", "on")
    SoundOnClick = true
else
    writefile("NovaGui/NovaGuiSettings/SoundsOnClick.nova", "off")
    SoundOnClick = false
end
end)

SettingsTab:Toggle("Notification On Button Click", function(bool)
if bool then
    writefile("NovaGui/NovaGuiSettings/NotifOnClick.nova", "on")
    NotifOnClick = true
else
    writefile("NovaGui/NovaGuiSettings/NotifOnClick.nova", "off")
    NotifOnClick = false
end
end)

SettingsTab:KeyBind("Ui Toggle KeyBind", function(val)
    if val == "W" then
        Notif("You Cannot Use W A S D.", 5)
        return
    elseif val == "A" then
        Notif("You Cannot Use W A S D.", 5)
        return
    elseif val == "S" then
        Notif("You Cannot Use W A S D.", 5)
        return
    elseif val == "D" then
        Notif("You Cannot Use W A S D.", 5)
        return
    elseif val == "E" then
        Notif("You Cannot Use E. ( E Is Used To Stomp. )", 5)
        return
    elseif val == "G" then
        Notif("You Cannot Use G. ( G Is Used To Grab. )", 5)
        return
    elseif val == "F" then
        Notif("You Cannot Use F. ( F Is Used To Block. )", 5)
        return
    elseif val == "R" then
        Notif("You Cannot Use R. ( R Is Used To Reload. )", 5)
        return
    end
    
    writefile("NovaGui/NovaGuiSettings/UIKeybind.nova", val)
    getgenv().UiKeybind = val
    Notif("Keybind Set To: " ..val, 10)
    
end)

if readfile("NovaGui/NovaGuiSettings/UIKeybind.nova", "") then
    print("No keybind found")
  else
    getgenv().UiKeybind = readfile("NovaGui/NovaGuiSettings/UIKeybind.nova")
end


