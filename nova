local locktarget = ""
TargetTab:TextBox("Username", false, function(val)
   -- checks if the box doesnt have any text so it doesnt bug and target random person
   if val == "" then
      return
   end

    locktarget = findtarget(val)
    Notif("Target: " ..locktarget)
end)

