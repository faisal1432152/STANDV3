bulletdeflectToggle = false
FunTab:Toggle("Bullet Deflect", function(bool)
if SoundOnClick == true then PlaySoundOnClick() end
    if bool then
        if not game:GetService("Players").LocalPlayer.Backpack:FindFirstChild("[Boombox]") then
            Notif("You must own the Boombox gamepass to use this.", 10)
            return
        end
        if game.Workspace.Players:FindFirstChild(game.Players.LocalPlayer.Name) then
            Notif("You must be using godv3 for this to work.", 10)
            return
        end
        Notif("This script can cause lag. When leaving the server or rejoining, your game may crash.")
        bulletdeflectToggle = true
    else
        bulletdeflectToggle = false
    end
end)

bulletdefAnims = false
FunTab:Toggle("Bullet Deflect Anims", function(bool)
if SoundOnClick == true then PlaySoundOnClick() end
    if bool then
        bulletdefAnims = true
    else
        bulletdefAnims = false 
    end
end)

    local Player = game.Players.LocalPlayer

local OriginalKeyUpValue = 0

local Swing1 = Instance.new('Animation', game.ReplicatedStorage)
Swing1.AnimationId = 'rbxassetid://8254794168'

local Swing2 = Instance.new('Animation', game.ReplicatedStorage)
Swing2.AnimationId = 'rbxassetid://8254792138'

local Swing3 = Instance.new('Animation', game.ReplicatedStorage)
Swing3.AnimationId = 'rbxassetid://8254789608'

local Swing4 = Instance.new('Animation', game.ReplicatedStorage)
Swing4.AnimationId = 'rbxassetid://8254787838'

local Anim1 = Player.Character:FindFirstChildOfClass('Humanoid'):LoadAnimation(Swing1)
local Anim2 = Player.Character:FindFirstChildOfClass('Humanoid'):LoadAnimation(Swing2)
local Anim3 = Player.Character:FindFirstChildOfClass('Humanoid'):LoadAnimation(Swing3)
local Anim4 = Player.Character:FindFirstChildOfClass('Humanoid'):LoadAnimation(Swing4)

function StopAudio()
    Player.Character.LowerTorso.BOOMBOXSOUND:Stop()
end

function stop(ID, Key)
    local cor = coroutine.wrap(function()
        wait(Player.Character.LowerTorso.BOOMBOXSOUND.TimeLength-0.2)
        if Player.Character.LowerTorso.BOOMBOXSOUND.SoundId == "rbxassetid://"..ID and OriginalKeyUpValue == Key then
            StopAudio()
        end
    end)
    cor()
end

function play(ID, STOP, LMAO)
    if Player.Backpack:FindFirstChild("[Boombox]") then
        local Tool = nil
        if Player.Character:FindFirstChildOfClass("Tool") and LMAO == true then
            Tool = Player.Character:FindFirstChildOfClass("Tool")
            Player.Character:FindFirstChildOfClass("Tool").Parent = Player.Backpack
        end
        Player.Backpack["[Boombox]"].Parent =
            Player.Character
        game.ReplicatedStorage.MainEvent:FireServer("Boombox", ID)
        Player.Character["[Boombox]"].RequiresHandle = false
        if Player.Character["[Boombox]"]:FindFirstChild("Handle") then
            Player.Character["[Boombox]"].Handle:Destroy()
        end
        Player.Character["[Boombox]"].Parent =
            Player.Backpack
        Player.PlayerGui.MainScreenGui.BoomboxFrame.Visible = false
        if Tool ~= true then
            if Tool then
                Tool.Parent = Player.Character
            end
        end
        if STOP == true then
            Player.Character.LowerTorso:WaitForChild("BOOMBOXSOUND")
            local cor = coroutine.wrap(function()
                repeat wait() until Player.Character.LowerTorso.BOOMBOXSOUND.SoundId == "rbxassetid://"..ID and Player.Character.LowerTorso.BOOMBOXSOUND.TimeLength > 0
                OriginalKeyUpValue = OriginalKeyUpValue+1
                stop(ID, OriginalKeyUpValue)
            end)
            cor()
        end
    end
end

randomsound = {
    "9113634070",
    "9113633912",
    "9113634061",
    "9113633916"
}

game.Workspace.Ignored.DescendantAdded:Connect(function(Ray)
    if Ray:IsA('Attachment') and bulletdeflectToggle == true then
        local mag = (Player.Character.PrimaryPart.Position - Ray.WorldPosition).Magnitude
        if mag <= 6 then
            local Math = math.random(1,4)
                play((randomsound[math.random(1,#randomsound)]), true, true)
              if bulletdefAnims == true then
                if Math == 1 then
                    Anim2:Stop()
                    Anim3:Stop()
                    Anim1:Play()
                    Anim4:Stop()
                    Anim1:AdjustSpeed(2.4)
                elseif Math == 2 then
                    Anim1:Stop()
                    Anim3:Stop()
                    Anim2:Play()
                    Anim4:Stop()
                    Anim2:AdjustSpeed(2.4)
                elseif Math == 3 then
                    Anim1:Stop()
                    Anim2:Stop()
                    Anim3:Play()
                    Anim4:Stop()
                    Anim3:AdjustSpeed(2.4)
                elseif Math == 4 then
                    Anim1:Stop()
                    Anim2:Stop()
                    Anim3:Stop()
                    Anim4:Play()
                    Anim4:AdjustSpeed(2.4)
                end
              end
        end
    end
end)

godv3weave = false
FunTab:Toggle("Godv3 Weave", function(bool)
if SoundOnClick == true then PlaySoundOnClick() end


if bool then
    
    if not game:GetService("Players").LocalPlayer.Backpack:FindFirstChild("[Boombox]") then
        Notif("You must own the Boombox gamepass to use this.", 10)
        return
    end
    
    if game.Workspace.Players:FindFirstChild(game.Players.LocalPlayer.Name) then
        Notif("You must be using godv3 for this to work.", 10)
        return
    end

    Notif("This script can cause lag. When leaving the server or rejoining, your game may crash.")
    
    local WeaveCooldown = 10
    godv3weave = true

local OriginalKeyUpValue = 0
function StopAudio()
	if game:GetService("Players").LocalPlayer.Character.LowerTorso:FindFirstChild("BOOMBOXSOUND") then
		game:GetService("Players").LocalPlayer.Character.LowerTorso.BOOMBOXSOUND:Stop()
	end
end
function stop(ID, Key)
	local cor = coroutine.wrap(function()
		wait(game:GetService("Players").LocalPlayer.Character.LowerTorso.BOOMBOXSOUND.TimeLength-0.1)
		if game:GetService("Players").LocalPlayer.Character.LowerTorso.BOOMBOXSOUND.SoundId == "rbxassetid://"..ID and OriginalKeyUpValue == Key then
			StopAudio()
		end
	end)
	cor()
end
function play(ID, STOP, LMAO)
	if game:GetService("Players").LocalPlayer.Backpack:FindFirstChild("[Boombox]") then
		local Tool = nil
		if game:GetService("Players").LocalPlayer.Character:FindFirstChildWhichIsA("Tool") and LMAO == true then
			Tool = game:GetService("Players").LocalPlayer.Character:FindFirstChildWhichIsA("Tool")
			game:GetService("Players").LocalPlayer.Character:FindFirstChildWhichIsA("Tool").Parent = game:GetService("Players").LocalPlayer.Backpack
		end
		game:GetService("Players").LocalPlayer.Backpack["[Boombox]"].Parent =
			game:GetService("Players").LocalPlayer.Character
		game.ReplicatedStorage.MainEvent:FireServer("Boombox", ID)
		game:GetService("Players").LocalPlayer.Character["[Boombox]"].RequiresHandle = false
		if game:GetService("Players").LocalPlayer.Character["[Boombox]"]:FindFirstChild("Handle") then
			game:GetService("Players").LocalPlayer.Character["[Boombox]"].Handle:Destroy()
		end
		game:GetService("Players").LocalPlayer.Character["[Boombox]"].Parent =
			game:GetService("Players").LocalPlayer.Backpack
		game:GetService("Players").LocalPlayer.PlayerGui.MainScreenGui.BoomboxFrame.Visible = false
		if Tool ~= true then
			if Tool then
				Tool.Parent = game:GetService("Players").LocalPlayer.Character
			end
		end
		if STOP == true then
			game:GetService("Players").LocalPlayer.Character.LowerTorso:WaitForChild("BOOMBOXSOUND")
			local cor = coroutine.wrap(function()
				repeat wait() until game:GetService("Players").LocalPlayer.Character.LowerTorso.BOOMBOXSOUND.SoundId == "rbxassetid://"..ID and game:GetService("Players").LocalPlayer.Character.LowerTorso.BOOMBOXSOUND.TimeLength > 0.01
				OriginalKeyUpValue = OriginalKeyUpValue+1
				stop(ID, OriginalKeyUpValue)
			end)
			cor()
		end
	end
end

local RandomWeave = {
    "2788314837",
    "2788312709",
    "2788313790",
    "2788316350",
    "2788315673"
}


local player = game.Players.LocalPlayer
local WeaveAnim1 = Instance.new("Animation")
WeaveAnim1.AnimationId = "rbxassetid://2788314837"
local WeaveAnimPlay1 = player.Character:WaitForChild("Humanoid"):LoadAnimation(WeaveAnim1)

local player = game.Players.LocalPlayer
local WeaveAnim2 = Instance.new("Animation")
WeaveAnim2.AnimationId = "rbxassetid://2788312709"
local WeaveAnimPlay2 = player.Character:WaitForChild("Humanoid"):LoadAnimation(WeaveAnim2)

local player = game.Players.LocalPlayer
local WeaveAnim3 = Instance.new("Animation")
WeaveAnim3.AnimationId = "rbxassetid://2788313790"
local WeaveAnimPlay3 = player.Character:WaitForChild("Humanoid"):LoadAnimation(WeaveAnim3)

local player = game.Players.LocalPlayer
local WeaveAnim4 = Instance.new("Animation")
WeaveAnim4.AnimationId = "rbxassetid://2788316350"
local WeaveAnimPlay4 = player.Character:WaitForChild("Humanoid"):LoadAnimation(WeaveAnim4)

local player = game.Players.LocalPlayer
local WeaveAnim5 = Instance.new("Animation")
WeaveAnim5.AnimationId = "rbxassetid://2788315673"
local WeaveAnimPlay5 = player.Character:WaitForChild("Humanoid"):LoadAnimation(WeaveAnim5)


                autoblocking = true
                alreadyon = false
                local MaxDistance = 10
                if 10 ~= nil then
                    MaxDistance = 10
                else
                    MaxDistance = 10
                end
                local forbidden = {
                    '[RPG]',
                    '[SMG]',
                    '[TacticalShotgun]',
                    '[AK47]',
                    '[AUG]',
                    '[Glock]',
                    '[Shotgun]',
                    '[Flamethrower]',
                    '[Silencer]',
                    '[AR]',
                    '[Revolver]',
                    '[SilencerAR]',
                    '[LMG]',
                    '[P90]',
                    '[DrumGun]',
                    '[Double-Barrel SG]',
                    '[Hamburger]',
                    '[Chicken]',
                    '[Pizza]',
                    '[Cranberry]',
                    '[Donut]',
                    '[Taco]',
                    '[Starblox Latte]',
                    '[BrownBag]',
                    '[Weights]',
                    '[HeavyWeights]',
                }
                local HoldingF = false
                local UIS = game:GetService('UserInputService')
                UIS.InputBegan:Connect(function(key, b)
                    if key.KeyCode == Enum.KeyCode.F and not b then
                        if alreadyon == true then
                            return
                        end
                        HoldingF = true
                        MainEvent:FireServer('Block', player.Name)
                    end
                end)
                UIS.InputEnded:Connect(function(key, b)
                    if key.KeyCode == Enum.KeyCode.F and not b then
                        if alreadyon == true then
                            return
                        end
                        HoldingF = false
                        if player.Character.BodyEffects:FindFirstChild('Block') then
                            player.Character.BodyEffects.Block:Destroy()
                        end
                    end
                end)
                local player = game.Players.LocalPlayer
                local MainEvent = game.ReplicatedStorage.MainEvent
                local Loop
                local loopFunction = function()
                    local success, problem = pcall(function()
                        local Found = false
                        for i,v in pairs(game.Workspace.Players:GetChildren()) do
                          if godv3weave == true then
                            local mag = (v.UpperTorso.Position - player.Character.HumanoidRootPart.Position).Magnitude
                            if mag <= MaxDistance then
                                if v.BodyEffects.Attacking.Value == true and not table.find(forbidden, v:FindFirstChildWhichIsA('Tool').Name ) and v.Name ~= player.Name then
                                    if alreadyon == true then
                                        return
                                    end
                                    Found = true
                                    MainEvent:FireServer('Block', player.Name)
                                    local RandomWeaveGuess = (RandomWeave[math.random(1,#RandomWeave)])
                                    wait()

                                    if alreadyon == false then
                                    alreadyon = true
                                    play(147722098, true, true)
                                    if RandomWeaveGuess == "2788314837" then
                                        WeaveAnimPlay1:Stop()
                                        WeaveAnimPlay1:Play()
                                    elseif RandomWeaveGuess == "2788312709" then
                                        WeaveAnimPlay2:Stop()
                                        WeaveAnimPlay2:Play()
                                    elseif RandomWeaveGuess == "2788313790" then
                                        WeaveAnimPlay3:Stop()
                                        WeaveAnimPlay3:Play()
                                    elseif RandomWeaveGuess == "2788316350" then
                                        WeaveAnimPlay4:Stop()
                                        WeaveAnimPlay4:Play()
                                    elseif RandomWeaveGuess == "2788315673" then
                                        WeaveAnimPlay5:Stop()
                                        WeaveAnimPlay5:Play()
                                    end
                                    end
         
                                    wait(WeaveCooldown)
                                    alreadyon = false
                                end
                            end
                        end
                        end
                        if Found == false and HoldingF == false then
                            if player.Character.BodyEffects:FindFirstChild('Block') then
                                player.Character.BodyEffects.Block:Destroy()
                            end
                        end
                    end)
                end;
                local Start = function()
                    Loop = game:GetService("RunService").Heartbeat:Connect(loopFunction);
                end;
                local Pause = function()
                    Loop:Disconnect()
                end;
                Start()
                repeat wait() until autoblocking == false
                Pause()
                

else
    
    godv3weave = false

end
end)

FunTab:TextBox("Godv3 Weave Cooldown", false, function(val)
  WeaveCooldown = val
end)

