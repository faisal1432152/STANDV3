            if control.w then
                vel = vel * CFrame.new(0, 0, -speed)
            end
            if control.s then
                vel = vel * CFrame.new(0, 0, speed)
            end
            if control.a then
                vel = vel * CFrame.new(-speed, 0, 0)
            end
            if control.d then
                vel = vel * CFrame.new(speed, 0, 0)
            end
            seat.Vel.Velocity = (seat.CFrame*vel).p - seat.CFrame.p
        end
    end
    if control.e and speed<100 then
        speed = speed + 1
    end
    if control.q and speed > 0 then
        speed = speed - 1
    end
end


