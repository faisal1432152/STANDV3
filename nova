LocalPlayerTab:Toggle("Auto Unjail", function(bool)
  if bool then
    autounjail = true
  else
    autounjail = false
  end
end)

unjailing = true
game:GetService('RunService').Stepped:connect(function()
if autounjail == true then
pcall(function()
    for _, v in next, game:GetService("Players").LocalPlayer.Character:FindFirstChildOfClass("Humanoid"):GetPlayingAnimationTracks() do
        if (v.Animation.AnimationId:match("rbxassetid://3144026747")) then
            repeat wait() until game:service'Players'.LocalPlayer.Character:findFirstChild("FULLY_LOADED_CHAR")
            if unjailing == false then
                return
            end
            unjailing = true
            wait()
            
            repeat wait()
              plr.Character.HumanoidRootPart.CFrame = game.Workspace.Ignored.Shop["[Key] - $125"].Head.CFrame
              fireclickdetector(game.Workspace.Ignored.Shop["[Key] - $125"].ClickDetector)
            until game.Players.LocalPlayer.Backpack:FindFirstChild("[Key]")

            wait(.1)
            if game.Players.LocalPlayer.Backpack:FindFirstChild("[Key]") then
                game.Players.LocalPlayer.Backpack:FindFirstChild("[Key]").Parent = game.Players.LocalPlayer.Character
                wait(.5)
                repeat wait() until game:service'Players'.LocalPlayer.Character:findFirstChild("FULLY_LOADED_CHAR")
                game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-319.2906494140625, 80.41372680664062, -210.28460693359375)
            end
            unjailing = false
        end;
    end;
end)
end
end)

