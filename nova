LocalPlayerTab:Toggle("Auto Block", function(bool)
if SoundOnClick == true then PlaySoundOnClick() end


   if bool then

if NotifOnClick == true then
    Notif("Auto Block Enabled!", 5)
end
                
                autoblocking = true
                local MaxDistance = 10
                if 10 ~= nil then
                    MaxDistance = 10
                else
                    MaxDistance = 10
                end
                local forbidden = {
                    '[RPG]',
                    '[SMG]',
                    '[TacticalShotgun]',
                    '[AK47]',
                    '[AUG]',
                    '[Glock]',
                    '[Shotgun]',
                    '[Flamethrower]',
                    '[Silencer]',
                    '[AR]',
                    '[Revolver]',
                    '[SilencerAR]',
                    '[LMG]',
                    '[P90]',
                    '[DrumGun]',
                    '[Double-Barrel SG]',
                    '[Hamburger]',
                    '[Chicken]',
                    '[Pizza]',
                    '[Cranberry]',
                    '[Donut]',
                    '[Taco]',
                    '[Starblox Latte]',
                    '[BrownBag]',
                    '[Weights]',
                    '[HeavyWeights]',
                }
                local HoldingF = false
                local UIS = game:GetService('UserInputService')
                UIS.InputBegan:Connect(function(key, b)
                    if key.KeyCode == Enum.KeyCode.F and not b then
                        HoldingF = true
                        MainEvent:FireServer('Block', player.Name)
                    end
                end)
                UIS.InputEnded:Connect(function(key, b)
                    if key.KeyCode == Enum.KeyCode.F and not b then
                        HoldingF = false
                        if player.Character.BodyEffects:FindFirstChild('Block') then
                            player.Character.BodyEffects.Block:Destroy()
                        end
                    end
                end)
                local player = game.Players.LocalPlayer
                local MainEvent = game.ReplicatedStorage.MainEvent
                local Loop
                local loopFunction = function()
                    local success, problem = pcall(function()
                        local Found = false
                        for i,v in pairs(game.Workspace.Players:GetChildren()) do
                            local mag = (v.UpperTorso.Position - player.Character.HumanoidRootPart.Position).Magnitude
                            if mag <= MaxDistance then
                                if v.BodyEffects.Attacking.Value == true and not table.find(forbidden, v:FindFirstChildWhichIsA('Tool').Name ) and v.Name ~= player.Name then
                                    Found = true
                                    MainEvent:FireServer('Block', player.Name)
                                end
                            end
                        end
                        if Found == false and HoldingF == false then
                            if player.Character.BodyEffects:FindFirstChild('Block') then
                                player.Character.BodyEffects.Block:Destroy()
                            end
                        end
                    end)
                end;
                local Start = function()
                    Loop = game:GetService("RunService").Heartbeat:Connect(loopFunction);
                end;
                local Pause = function()
                    Loop:Disconnect()
                end;
                Start()
                repeat wait() until autoblocking == false
                Pause()
                
   else
      autoblocking = false

if NotifOnClick == true then
    Notif("Auto Block Disabled!", 5)
end
   end
end)

