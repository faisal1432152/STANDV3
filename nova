		local lastpos = plr.Character.HumanoidRootPart.CFrame					
		plr.Character.HumanoidRootPart.CFrame = game.Workspace.Ignored.Shop["[High-Medium Armor] - $2300"].Head.CFrame
		wait(.3)
		fireclickdetector(game.Workspace.Ignored.Shop["[High-Medium Armor] - $2300"].ClickDetector)
		game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(autohigharmorpos)
	end
	until autohigharmour == false
else
    if SoundOnClick == true then PlaySoundOnClick() end


if NotifOnClick == true then
    Notif("Auto High Armor Disabled!", 5)
end
	autohigharmour = false
end
