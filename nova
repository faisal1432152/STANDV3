game:service"RunService".RenderStepped:connect(function()
    if loopkill == true and game.Players[locktarget].Character.BodyEffects["K.O"].Value == false then
        
        if knifeattackmode == false then
        if game.Players.LocalPlayer.Character:FindFirstChild('Combat') then
            if lightattackmode == false then
                game.Players.LocalPlayer.Character:FindFirstChild('Combat'):Activate()
            elseif lightattackmode == true then
                game.Players.LocalPlayer.Character:FindFirstChild('Combat'):Activate()
                game.Players.LocalPlayer.Character:FindFirstChild('Combat'):Deactivate()
            end
        else
            game.Players.LocalPlayer.Backpack:FindFirstChild('Combat').Parent = game.Players.LocalPlayer.Character
        end 
        elseif knifeattackmode == true then
            		for i, v in pairs(game:GetService "Players".LocalPlayer.Character:GetChildren()) do
			if v:isA("Tool") then
				v.Handle.Size = Vector3.new(60, 60, 60)
				v.Handle.Transparency = 1
         end
      end
            if game.Players.LocalPlayer.Character:FindFirstChild("[Knife]") or game.Players.LocalPlayer.Backpack:FindfirstChild("[Knife]") then
                       if game.Players.LocalPlayer.Character:FindFirstChild('[Knife]') then
            if lightattackmode == false then
                game.Players.LocalPlayer.Character:FindFirstChild('[Knife]'):Activate()
            elseif lightattackmode == true then
                game.Players.LocalPlayer.Character:FindFirstChild('[Knife]'):Activate()
                game.Players.LocalPlayer.Character:FindFirstChild('[Knife]'):Deactivate()
            end
        else
            game.Players.LocalPlayer.Backpack:FindFirstChild('Combat').Parent = game.Players.LocalPlayer.Character
        end 
            end
        end
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(game:GetService("Players")[locktarget].Character.UpperTorso.Position) * CFrame.new(0,-9,0)
        game.Workspace.Camera.CameraSubject = game:GetService("Players")[locktarget].Character.Humanoid
    else
        if loopkill == true and game.Players[locktarget].Character.BodyEffects["K.O"].Value == true then
        if game.Players[locktarget].Character.BodyEffects.Dead.Value == false then
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(game:GetService("Players")[locktarget].Character.UpperTorso.Position) * CFrame.new(0,3,0)
            game.Workspace.Camera.CameraSubject = game:GetService("Players")[locktarget].Character.Humanoid
            game.ReplicatedStorage.MainEvent:FireServer("Stomp")

            if ezspam == true then
              pcall(function()
                wait(1)
                local A_1 = (loopkilltoxicmsg[math.random(1,#loopkilltoxicmsg)]) local A_2 = "All" local Event = game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest Event:FireServer(A_1, A_2)
              end)
            end

            else
              game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(0,math.random(10000,100000),math.random(10000,100000))
            end
        end
    end
end)
--END

