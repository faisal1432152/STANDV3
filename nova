   function GlideOG()
local GlideTool = Instance.new("Tool", game.Players.LocalPlayer.Backpack)
GlideTool.Name = "Glide"
GlideTool.Parent = game.Players.LocalPlayer.Backpack
GlideTool.RequiresHandle = false

local hoh = Instance.new("Animation", game.Workspace)
hoh.Name = "Fly"
hoh.AnimationId = "rbxassetid://3084858603"

--wait();
local l__LocalPlayer__1 = game.Players.LocalPlayer;
while true do
    wait();
    if l__LocalPlayer__1.Character then
        break;
    end;
end;
local l__Character__2 = l__LocalPlayer__1.Character;
local l__Humanoid__3 = l__Character__2:WaitForChild("Humanoid");
local l__Parent__4 = GlideTool
local u1 = false;
local u2 = require(game.ReplicatedStorage.MainModule);
local u3 = false;
local u4 = l__Humanoid__3:LoadAnimation(game.Workspace.Fly);
l__Parent__4.Equipped:connect(function(p1)
    p1.Button1Down:connect(function()
        if u1 or l__Humanoid__3.FloorMaterial ~= Enum.Material.Air and u2.Activate(l__Character__2.BodyEffects) ~= true then
            return;
        end;
        u1 = true;
        u3 = true;
        --on
        u4:Play();
        local v5 = Instance.new("BodyVelocity", l__Character__2.Head);
        v5.MaxForce = Vector3.new(50000, 50000, 50000);
        v5.Velocity = (l__Character__2.Head.CFrame * CFrame.Angles(math.rad(-18), 0, 0)).lookVector * 40;
        while u3 and l__Humanoid__3.FloorMaterial == Enum.Material.Air do
            v5.Velocity = (l__Character__2.Head.CFrame * CFrame.Angles(math.rad(-18), 0, 0)).lookVector * 40;
            game:GetService("RunService").Heartbeat:wait();        
        end;
        v5:Destroy();
        --off
        u3 = false;
        u4:Stop();
        wait(0.3);
        u1 = false;
    end);
    p1.Button1Up:connect(function()
        u3 = false;
    end);
end);
l__Parent__4.Unequipped:connect(function()
    u3 = false;
end);
end
game.Players.LocalPlayer.Character:WaitForChild("FULLY_LOADED_CHAR")
wait(1)
GlideOG()
game.Players.LocalPlayer.CharacterAdded:connect(function()
	game.Players.LocalPlayer.Character:WaitForChild("FULLY_LOADED_CHAR")
	wait(1)
	GlideOG()
end)

