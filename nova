LocalPlayerTab:Button("Free Fist", function()

if SoundOnClick == true then PlaySoundOnClick() end


Notif("Keybind is 'q'", 10)

-- freefist
local localPlayer = game:GetService("Players").LocalPlayer;
local localCharacter = localPlayer.Character
local Mouse = localPlayer:GetMouse();
local FistControl = false
local LeftFist = localCharacter.LeftHand;
local RightFist = localCharacter.RightHand;

local uis = game:GetService("UserInputService");
local loopFunction = function()
	LeftFist.CFrame  = CFrame.new(Mouse.Hit.p);
	RightFist.CFrame = CFrame.new(Mouse.Hit.p);
end;
 
local Loop
local Start = function()
	Loop = game:GetService("RunService").Heartbeat:Connect(loopFunction);
end;

local Pause = function()
	Loop:Disconnect()
end;

uis.InputBegan:connect(function(Key)
	if (Key.KeyCode == Enum.KeyCode.Q) then
		if (FistControl == false) then
			FistControl = true;
			Start();
			pcall(function()
				localCharacter.RightHand.RightWrist:Remove();
				localCharacter.LeftHand.LeftWrist:Remove();
			end);
		elseif (FistControl == true) then
			FistControl = false;
			Pause();
			local LocalPlayer = game:GetService("Players").LocalPlayer
			local char = LocalPlayer.Character
			LocalPlayer.Character.RightHand.CFrame = LocalPlayer.Character.HumanoidRootPart.CFrame
			LocalPlayer.Character.LeftHand.CFrame = LocalPlayer.Character.HumanoidRootPart.CFrame
			
			local NewLeftWrist = Instance.new("Motor6D")
            NewLeftWrist.Name = "LeftWrist"
            NewLeftWrist.Parent = game.Players.LocalPlayer.Character.LeftHand
            NewLeftWrist.C0 = CFrame.new(-0.03779025, -0.251719505, 0.00293661049, 1, 0, 0, 0, 1, 0, 0, 0, 1)
            NewLeftWrist.C1 = CFrame.new(0.0364160053, 0.443639606, 0.0604552627, 1, 0, 0, 0, 1, 0, 0, 0, 1)
            NewLeftWrist.Part0 = game.Players.LocalPlayer.Character.LeftLowerArm
            NewLeftWrist.Part1 = game.Players.LocalPlayer.Character.LeftHand
            local NewRightWrist = Instance.new("Motor6D")
            NewRightWrist.Name = "RightWrist"
            NewRightWrist.Parent = game.Players.LocalPlayer.Character.RightHand
            NewRightWrist.C0 = CFrame.new(1.78813934e-07, -0.651207268, -1.02936713e-17, 1, 0, 0, 0, 1, 0, 0, 0, 1)
            NewRightWrist.C1 = CFrame.new(5.36441803e-07, 0.162559018, 8.94069672e-08, 1, 0, 0, 0, 1, 0, 0, 0, 1)
            NewRightWrist.Part0 = game.Players.LocalPlayer.Character.RightLowerArm
            NewRightWrist.Part1 = game.Players.LocalPlayer.Character.RightHand

		end;
	end;
end);


end)

