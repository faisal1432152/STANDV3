        wait(.2)
        Fall:Play()
        -- slow fall
        player.Character.HumanoidRootPart.Anchored = false
        local Character = game.Players.LocalPlayer.Character
        local bd = Instance.new("BodyVelocity", Character.Head);
        bd.Name = "SlowFallVelocity"
        bd.MaxForce = Vector3.new(2300, 2300, 2300);
        bd.Velocity = (Character.Head.CFrame * CFrame.Angles(math.rad(2), 0, 0)).lookVector * 0;
        wait(1)
		bd:Destroy()
        -- yeh yeaa
        checkfoot = true
        game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.Touched:connect(function()
            if checkfoot == true then
                checkfoot = false
			    Fall:Stop()
            end
        end)
	    end
