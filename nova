                local wheel = seat.Parent:FindFirstChild("RTWheel")
                wheel:remove()
            local wheel = seat.Parent:FindFirstChild("RBWheel")
                wheel:remove()
                            local wheel = seat.Parent:FindFirstChild("LBWheel")
                wheel:remove()
                            local wheel = seat.Parent:FindFirstChild("LTWheel")
                wheel:remove()
            local gyro = Instance.new("BodyGyro", seat)
            gyro.Name = "Gyro"
            local pos = Instance.new("BodyVelocity", seat)
            pos.Name = "Vel"
            gyro.maxTorque = Vector3.new(9e9, 9e9, 9e9)
            pos.MaxForce = Vector3.new(9e9, 9e9, 9e9)
        else 
            seat.Gyro.cframe = workspace.CurrentCamera.CoordinateFrame
