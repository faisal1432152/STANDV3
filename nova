-- soundonclick audio id yeah
function PlaySoundOnClick()
    local SoundClick = Instance.new("Sound", game.CoreGui)
    SoundClick.SoundId = "rbxassetid://4307186075"
    SoundClick.PlayOnRemove = true
    SoundClick.Volume = 0.5
    SoundClick:Destroy()
end

